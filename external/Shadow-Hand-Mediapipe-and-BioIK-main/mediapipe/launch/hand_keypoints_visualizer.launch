<launch>
    <!-- Node [Human Hand Keypoints] -->
    <node name="human_hand_keypoints_visualizer" pkg="mediapipe" type="hand_keypoints_visualizer.py" output="screen">
        <!-- Set Parameters -->
        <param name="human" value="True" />
    </node>
    <!-- Node [Shadow Hand Keypoints] -->
    <node name="shadow_hand_keypoints_visualizer" pkg="mediapipe" type="hand_keypoints_visualizer.py" output="screen">
        <!-- Set Parameters -->
        <param name="human" value="False" />
    </node>
</launch>
